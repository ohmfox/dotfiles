alias vim=nvim
alias g=hub
alias bs="source ~/.bashrc"
alias tmux="tmux -2"

    function up {
        if [[ "$#" < 1 ]] ; then
            cd ..
        else
            CDSTR=""
            for i in {1..$1} ; do
                CDSTR="../$CDSTR"
            done
            cd $CDSTR
        fi
    }

